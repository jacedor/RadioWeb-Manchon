@section barraTop{
    <form id="filtrosBusqueda" method="get">

        <div class="border-bottom white-bg dashboard-header">
            <div class="row">
                <div class="col-sm-4 ">
                    <div class="form-group">
                        <label class="control-label ">Fecha</label>
                        <div class="input-group date">
                            <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                            @{
                                string FechaActual = "";
                                if (Session["FiltrosBusqueda"] != null)
                                {
                                    FechaActual = oFiltros.Fecha;
                                }
                                else
                                {
                                    FechaActual = DateTime.Now.ToString("dd-MM-yyyy");

                                }

                            }
                            <input id="fechaSelect" type="text" class="form-control" value="@FechaActual">
                        </div>
                    </div>

                </div>

                <div class="col-sm-4">
                    <div class="form-group">
                        <label class="control-label">Aparato</label>
                        @{
                            IEnumerable<RadioWeb.Models.DAPARATOS> oListTemp = ((List<RadioWeb.Models.DAPARATOS>)ViewBag.Daparatos);

                            if (Session["FiltrosBusqueda"] != null && oFiltros.oidCentro > 0)
                            {
                                oListTemp = oListTemp.Where(s => s.CID == oFiltros.oidCentro);
                            }

                            <select id="ddlAparatos" name="ddlAparatos" class="form-control select2" data-filter-calendar="true">
                                @if (Session["FiltrosBusqueda"] != null && oFiltros.oidCentro > 0)
                                {
                                    <option value="-1">&nbsp;</option>
                                }

                                @foreach (RadioWeb.Models.DAPARATOS item in oListTemp)
                                {
                                    if (!item.COD_FIL.Contains("OBSOLETO"))
                                    {
                                        if (Session["FiltrosBusqueda"] != null && item.OID == oFiltros.oidAparato)
                                        {
                                            <option selected="selected" value="@item.OID">@item.COD_FIL</option>
                                        }
                                        else
                                        {
                                            <option value="@item.OID">@item.COD_FIL</option>
                                        }

                                    }

                                }
                            </select>
                        }


                    </div>
                </div>
                <div class="col-sm-4">
                    <div class="form-group">
                        <label class="control-label">Grupo</label>

                        @Html.DropDownList("ddlGrupo", (IEnumerable<SelectListItem>)ViewBag.GrupoAparatos, new { data_filter_calendar = "true", @class = "form-control select2 ", style = "padding:4px;" })

                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-4">
                    <div class="form-group">
                        <label class="control-label">Mutua</label>
                        @{
                            List<RadioWeb.Models.MUTUAS> oListTempMutuas = (List<RadioWeb.Models.MUTUAS>)System.Web.HttpContext.Current.Application["Mutuas"];

                            <select id="ddlMutuas" class="select2  form-control " data-filter-calendar="true" data-live-search="true" style="padding: 4px;">
                                @foreach (RadioWeb.Models.MUTUAS item in oListTempMutuas)
                                {
                                    if (Session["FiltrosBusqueda"] != null && item.OID == oFiltros.oidMutua)
                                    {
                                        <option data-subtext="@item.NOMBRE" selected="selected" data-text-value="@item.CODMUT" value="@item.OID">
                                            @if (System.Configuration.ConfigurationManager.AppSettings["ComboMutuas"].ToUpper() == "NOMBRE")
                                            {
                                                /**/
                                                @item.NOMBRE
                                            }
                                            else
                                            {
                                                @item.CODMUT
                                            }

                                        </option>
                                    }
                                    else
                                    {
                                        <option data-subtext="@item.NOMBRE" data-text-value="@item.CODMUT" value="@item.OID">
                                            @if (System.Configuration.ConfigurationManager.AppSettings["ComboMutuas"].ToUpper() == "NOMBRE")
                                            {
                                                /**/
                                                @item.NOMBRE
                                            }
                                            else
                                            {
                                                @item.CODMUT
                                            }
                                        </option>
                                    }

                                }
                            </select>
                        }

                    </div>
                </div>
                <div class="col-sm-4">
                    <div class="form-group">
                        <label class="control-label">Centro</label>

                        @{
                            List<RadioWeb.Models.CENTROS> oListCentros = (List<RadioWeb.Models.CENTROS>)ViewBag.Centros;
                            <select id="ddlCentros" class="select2 form-control " data-filter-calendar="true" data-live-search="true" style="padding: 4px;">
                                @foreach (RadioWeb.Models.CENTROS item in oListCentros)
                                {
                                    if (Session["FiltrosBusqueda"] != null && item.OID == oFiltros.oidCentro)
                                    {
                                        <option selected="selected" value="@item.OID">@item.NOMBRE </option>
                                    }
                                    else
                                    {
                                        <option value="@item.OID">@item.NOMBRE </option>
                                    }

                                }
                            </select>
                        }

                    </div>
                </div>
                <div class="col-sm-2">
                    <div class="form-group">
                        <label class="control-label">Tipo</label>

                        <select id="ddlTipoExploracion" class="selectpicker form-control select2" data-filter-calendar="true" data-live-search="true" style="padding: 4px;">
                            <option value="-1">&nbsp;</option>
                            <option value="1">PRI</option>
                            <option value="2">MUT</option>
                            <option value="3">ICS</option>
                        </select>

                    </div>
                </div>
                <div class="col-sm-1 ">
                    <div class="form-group">
                        <label class="col-sm-2 control-label">Huecos</label>
                        <div class="col-sm-10">
                            @if (Session["FiltrosBusqueda"] != null && oFiltros.SoloHuecos == "true")
                            {
                                <input type="checkbox" id="chkHuecos" name="chkHuecos" class="js-switch" checked />
                            }
                            else
                            {
                                <input type="checkbox" id="chkHuecos" name="chkHuecos" class="js-switch" />
                            }

                        </div>
                    </div>

                </div>

                <div class="col-sm-1 ">
                    <div class="form-group">
                        <label class="col-sm-2 control-label">Borrados</label>
                        <div class="col-sm-10">
                            @if (Session["FiltrosBusqueda"] != null && oFiltros.Borrados == "true")
                            {
                                <input type="checkbox" id="chkBorrados" name="chkBorrados" class="js-switch" checked />
                            }
                            else
                            {
                                <input type="checkbox" class="js-switch" id="chkBorrados" name="chkBorrados" />
                            }

                        </div>
                    </div>

                </div>
            </div>

        </div>
    </form>

}


