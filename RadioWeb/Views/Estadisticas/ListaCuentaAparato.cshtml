@model IEnumerable<RadioWeb.Models.Estadistica.ItemEstadisticoExploraciones>

<table class="table">
    <tbody>

        @{
           
            
            foreach (var item in Model)
            {
         
            <tr>
                <td>
                    <button class="btn m-r-sm botonAparato"  style="background-color:@item.Color" data-aparato="@item.Aparato" type="button"><span style="color: white;">@item.Valor</span></button>
                    @item.Aparato
                </td>

            </tr>
           
            }
        }


    </tbody>
</table>
