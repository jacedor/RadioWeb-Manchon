@model IEnumerable<RadioWeb.Models.EXPLORACION>
@{
   
    RadioWeb.Models.USUARIO oUsuarioLogeado = (RadioWeb.Models.USUARIO)Session["Usuario"];
    ViewBag.Title = "Buscar Paciente";
    RadioWeb.Models.Logica.FiltrosBusquedaExploracion oFiltros = (RadioWeb.Models.Logica.FiltrosBusquedaExploracion)Session["FiltrosBusqueda"];
}


<div class="ibox">
    <div class="ibox-title">
        <h5>
            @foreach (var item in Model) { 
          <span class="label label-sucess">@item.FECHA.Value.ToShortDateString() @item.HORA @item.DAPARATO.COD_FIL</span>
            }
            


        </h5>
        
        <div class="ibox-tools">
            <a class="btn btn-primary btn-xs" href="/AgendaMulti/AddPaso2/0">Crear paciente nuevo</a>
        </div>
    </div>
    <div class="ibox-content" style="min-height: 300px;">
        <div class="row m-b-sm m-t-sm">

            <div class="col-md-12">
                <div class="input-group">
                    <input type="text" class="input-sm form-control" id="pacienteFilter" name="pacienteFilter" placeholder="Buscar Paciente" style="text-transform: uppercase;">
                    <span class="input-group-btn">
                        <button class="btn btn-sm btn-primary" id="BuscarPaciente" type="button">Buscar</button>
                    </span>
                </div>
            </div>
        </div>

        <div class="project-list">
            <div id="spiner-cargando-pacientes" class="row hide" style="height: 600px;">

                <div class="sk-spinner sk-spinner-circle" style="margin-top: 60px; height: 75px; width: 70px;">
                    <div class="sk-circle1 sk-circle"></div>
                    <div class="sk-circle2 sk-circle"></div>
                    <div class="sk-circle3 sk-circle"></div>
                    <div class="sk-circle4 sk-circle"></div>
                    <div class="sk-circle5 sk-circle"></div>
                    <div class="sk-circle6 sk-circle"></div>
                    <div class="sk-circle7 sk-circle"></div>
                    <div class="sk-circle8 sk-circle"></div>
                    <div class="sk-circle9 sk-circle"></div>
                    <div class="sk-circle10 sk-circle"></div>
                    <div class="sk-circle11 sk-circle"></div>
                    <div class="sk-circle12 sk-circle"></div>
                </div>


            </div>
            <table id="PacienteList" class="table table-hover">
                <tbody>
                    @*@foreach (var item in Model)
                    {

                    }*@
                </tbody>
            </table>
        </div>
    </div>
</div>

@section scripts{
    <script src="~/js/custom/AgendaMultiple/Alta.js"></script>
}